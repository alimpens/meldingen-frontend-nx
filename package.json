{
  "name": "meldingen-frontend",
  "version": "0.0.0",
  "description": "Frontends for the Signalen applications",
  "author": "Gemeente Amsterdam",
  "license": "EUPL-1.2",
  "private": true,
  "engines": {
    "node": "^20",
    "npm": "^10",
    "yarn": "please-use-npm",
    "pnpm": "please-use-npm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alimpens/meldingen-frontend-nx.git"
  },
  "scripts": {
    "build": "nx run-many --target=build",
    "build:admin": "nx build admin",
    "build:public": "nx build public",
    "dev": "nx run-many --parallel --target=dev --projects=admin,public",
    "dev:admin": "nx dev admin",
    "dev:public": "nx dev public",
    "lint": "nx run-many --target=lint",
    "lint:css": "stylelint --allow-empty-input '**/*.{css,scss}'",
    "lint:md": "markdownlint **/*.md --ignore node_modules",
    "lint-fix": "nx run-many --target=lint --fix",
    "lint-fix:css": "stylelint --fix '**/*.{css,scss}'",
    "lint-fix:md": "markdownlint --fix **/*.md",
    "prepare": "husky",
    "prettier": "prettier --write ."
  },
  "dependencies": {
    "@amsterdam/design-system-assets": "0.2.0",
    "@amsterdam/design-system-css": "0.7.0",
    "@amsterdam/design-system-react": "0.7.0",
    "@amsterdam/design-system-tokens": "0.7.0",
    "@formio/js": "5.0.0-rc.39",
    "@formio/react": "5.3.0-rc.3",
    "next": "14.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tslib": "2.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.14.5",
    "@babel/preset-react": "7.14.5",
    "@nx/eslint": "18.0.8",
    "@nx/eslint-plugin": "18.0.8",
    "@nx/jest": "18.0.8",
    "@nx/js": "18.0.8",
    "@nx/next": "18.0.8",
    "@nx/react": "18.0.8",
    "@nx/vite": "18.0.8",
    "@nx/web": "18.0.8",
    "@nx/workspace": "18.0.8",
    "@swc-node/register": "1.8.0",
    "@swc/core": "1.3.85",
    "@swc/helpers": "0.5.2",
    "@testing-library/react": "14.0.0",
    "@types/jest": "29.4.0",
    "@types/node": "18.16.9",
    "@types/react": "18.2.33",
    "@types/react-dom": "18.2.14",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.2.0",
    "@vitejs/plugin-react": "4.2.0",
    "@vitest/coverage-v8": "1.0.4",
    "@vitest/ui": "1.3.1",
    "babel-jest": "29.4.1",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "9.0.11",
    "jest": "29.4.1",
    "jest-environment-jsdom": "29.4.1",
    "jsdom": "22.1.0",
    "lint-staged": "15.2.2",
    "markdownlint-cli": "0.39.0",
    "nx": "18.0.8",
    "prettier": "2.6.2",
    "stylelint": "16.2.1",
    "stylelint-config-css-modules": "4.4.0",
    "stylelint-config-standard": "36.0.0",
    "stylelint-order": "6.0.4",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.3.2",
    "vite": "5.0.0",
    "vite-plugin-dts": "2.3.0",
    "vitest": "1.3.1"
  }
}
